<%layout("/layouts/boilerplate") %>
<body>
  <h1>Listings</h1>

  <!-- <form action="/listings/new" method="GET">
    <button>Add New Listing</button>
  </form> -->
  <div class="row row-cols-md-2 row-cols-1 row-cols-lg-3">
    <%for(val of data){%>
    <div class="card col">
      <a href="/listings/<%=val._id%>">
        <img src="<%=val.image%>" class="card-img-top" alt="..." />
        <div class="card-img-overlay"></div>
        <div class="card-body">
          <p class="card-text">
            <strong> <%=val.title%></strong>
            <br />PKR <%=val.price.toLocaleString("en-IN") %>/night
          </p>
        </div>
        <!-- https://picsum.photos/200/300 -->
      </a>
    </div>
    <%}%>
  </div>
</body>
